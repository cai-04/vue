div class=\"input-tip\">\n                    商品名称:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParams.proName\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    用户名称:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParams.userName\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    手机号码:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParams.phone\"></el-input>\n                </div>                \n            </el-container>\n            <el-container class=\"content-row\">\n                <div class=\"input-tip\">\n                    订单类型:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParams.type\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    平台单号:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParams.id\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    支付时间:\n                </div>\n                <div class=\"input-field\">\n                    <el-date-picker\n                    type=\"daterange\"\n                    range-separator=\"至\"\n                    start-placeholder=\"开始日期\"\n                    end-placeholder=\"结束日期\"\n                    v-model=\"queryParams.payTime\">\n                    </el-date-picker>\n                </div>             \n            </el-container>\n            <el-container class=\"content-row\">\n                <el-button type=\"primary\" @click=\"search\">搜索</el-button>\n                <el-button type=\"primary\" @click=\"clear\">清空搜索条件</el-button>\n                <el-button type=\"primary\" @click=\"exportData\">导出</el-button>\n            </el-container>\n        </div>\n        <div>\n            <el-table\n            :data=\"tradeList\"\n            tooltip-effect=\"dark\"\n            style=\"width: 100%\">\n                <el-table-column\n                label=\"名称\"\n                width=\"400\"\n                prop=\"name\">\n                </el-table-column>\n                <el-table-column\n                label=\"单号\"\n                width=\"150\"\n                prop=\"id\">\n                </el-table-column>\n                <el-table-column\n                label=\"用户\"\n                width=\"100\"\n                prop=\"user\">\n                </el-table-column>\n                <el-table-column\n                label=\"支付方式\"\n                width=\"100\"\n                prop=\"payType\">\n                </el-table-column>\n                <el-table-column\n                label=\"时间\"\n                width=\"300\"\n                prop=\"time\">\n                </el-table-column>\n            </el-table>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Mock from '../../mock/Mock'\nimport Tools from '../../tools/Tools'\nexport default {\n    data() {\n        return {\n            queryParams:{\n                proName:\"\",\n                userName:\"\",\n                phone:\"\",\n                type:\"\",\n                id:\"\",\n                payTime:\"\"\n            },\n            tradeList:[]\n        }\n    },\n    mounted() {\n        this.tradeList = Mock.getTradeInfo();\n    },\n    methods: {\n        search() {\n            this.$message({\n                type:'success',\n                message:'请求参数：' + JSON.stringify(this.queryParams)\n            });\n            this.tradeList = Mock.getTradeInfo();\n        },\n        clear() {\n            this.queryParams = {\n                proName:\"\",\n                userName:\"\",\n                phone:\"\",\n 