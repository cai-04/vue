-input v-model=\"queryParams.viewCount\"></el-input>\n            </div>\n        </el-container>\n        <el-container class=\"content-row\">\n            <el-button type=\"success\" plain @click=\"submit\">提交</el-button>\n            <div style=\"margin-left:40px\"></div>\n            <el-button type=\"warning\" plain @click=\"cancel\">取消</el-button>\n        </el-container>\n    </div>\n</template>\n\n<script>\nexport default {\n    data () {\n        return {\n            queryParams: {\n                maketPrice:0,\n                showPrice:0,\n                coin:0,\n                price:0,\n                limit:0,\n                count:0,\n                sellCount:0,\n                viewCount:0\n            }\n        }\n    },\n    methods: {\n        cancel(){\n            this.$router.go(-1);\n        },\n        submit() {\n            this.$message({\n                type:'success',\n                message:'设置价格与库存：' + JSON.stringify(this.queryParams)\n            })\n        }\n    }\n}\n</script>\n\n<style scoped>\n.title {\n    background-color:#e1e1e1;\n    height: 35px;\n    margin-bottom: 15px;\n}\n</style>"]}]}