0BANlB,EAMkB;AALlB,QAAA,KAAK,EAAC,MAKY;AAJlB,QAAA,KAAK,EAAC;AAIY,OANlB,E;AAGe,QAAA,OAAO,EAAA,QAAA,CACd,UADqB,KACrB;AAAA,iBADqB,CACrB,YAAA,CAAqE,iBAArE,EAAqE;AAA7D,YAAA,IAAI,EAAC;AAAwD,WAArE,EAAqB;8BAAC;AAAA,qBAAsC,C,kCAAnC,KAAK,CAAC,GAAN,CAAU,OAAV,GAAiB,IAAjB,GAAiB,K,GAAA,C,CAAkB,CAAtC;AAAA,a,CAAD;;AAAA,WAArB,E,IAAA,CADqB,CACrB;AAAA,SADc,C;;OAHtB,CA5CkB,EAmDlB,YAAA,CAIkB,0BAJlB,EAIkB;AAHlB,QAAA,KAAK,EAAC,IAGY;AAFlB,QAAA,KAAK,EAAC,KAEY;AADlB,QAAA,IAAI,EAAC;AACa,OAJlB,CAnDkB,CAHlB;AAAA,K;;GANJ,E,CAAA,E,6BAAA,CAkEE,CA3EN,CA6EE,CA/IN,C","sourcesContent":["<template>\n    <div class=\"content-container\" direction=\"vertical\">\n        <!-- input -->\n        <div>\n            <el-container class=\"content-row\">\n                <div class=\"input-tip\">\n                    商品名称:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParam.good\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    收货人:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParam.consignee\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    支付时间:\n                </div>\n                <div class=\"input-field\">\n                    <el-date-picker\n                    type=\"daterange\"\n                    range-separator=\"至\"\n                    start-placeholder=\"开始日期\"\n                    end-placeholder=\"结束日期\"\n                    v-model=\"queryParam.payTime\">\n                    </el-date-picker>\n                </div>\n            </el-container>\n            <el-container class=\"content-row\">\n                <div class=\"input-tip\">\n                    用户名称:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParam.name\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    手机号:\n                </div>\n                <div class=\"input-field\">\n                    <el-input v-model=\"queryParam.phone\"></el-input>\n                </div>\n                <div class=\"input-tip\">\n                    发货时间:\n                </div>\n                <div class=\"input-field\">\n                    <el-date-picker\n                    type=\"daterange\"\n                    range-separator=\"至\"\n                    start-placeholder=\"开始日期\"\n                    end-placeholder=\"结束日期\"\n                    v-model=\"queryParam.sendTime\">\n                    </el-date-picker>\n                </div>                \n            </el-container>\n        </div>\n        <div class=\"content-row\">\n            <el-container>\n                <el-button type=\"primary\" @click=\"requestData\">筛选</el-button>\n                <el-button type=\"danger\" @click=\"clear\">清空筛选</el-button>\n                <el-button type=\"primary\" @click=\"exportData\">导出</el-button>\n                <el-button type=\"primary\" @click=\"dispatchGoods\">批量发货</el-button>\n                <el-button type=\"primary\" @click=\"exportDispatchGoods\">下载批量发货样单</el-button>\n            </el-container>\n        </div>\n        <!-- list -->\n        <div>\n            <el-tabs type=\"card\" @tab-click=\"handleClick\">\n                <el-tab-pane label=\"全部\"></el-tab-pane>\n                <el-tab-pane label=\"未支付\"></el-tab-pane>\n                <el-tab-pane label=\"已支付\"></el-tab-pane>\n                <el-tab-pane label=\"待发货\"></el-tab-pane>\n                <el-tab-pane label=\"已发货\"></el-tab-pane>\n                <el-tab-pane label=\"支付超时\"></el-tab-pane>\n            </el-tabs>\n            <el-table\n            ref=\"multipleTable\"\n            :data=\"orderList\"\n            tooltip-effect=\"dark\"\n            style=\"width: 100%\"\n            @selection-change=\"handleSelectionChange\">\n                <el-table-column\n                type=\"selection\"\n                width=\"55\">\n                </el-table-column>\n                <el-table-column\n                label=\"商品\"\n                width=\"100\"\n                prop=\"name\">\n                </el-table-column>\n                <el-table-column\n                label=\"总价/数量\"\n                width=\"100\"\n                prop=\"price\">\n                </el-table-column>\n                <el-table-column\n                label=\"买家信息\"\n                width=\"100\"\n                prop=\"buyer\">\n                </el-table-column>\n                <el-table-column\n                label=\"交易时间\"\n                width=\"200\"\n                prop=\"time\">\n                </el-table-column>\n                <el-table-column\n                label=\"分销信息\"\n                width=\"100\"\n                >\n                    <template #default=\"scope\">\n                        <el-tag size=\"medium\" :type=\"scope.row.role ? '' : 'info'\">{{ scope.row.role ? '经理' : '分销员' }}</el-tag>          \n                    </template>\n                </el-table-column>\n                <el-table-column\n                label=\"状态\"\n                width=\"100\">\n                    <template #default=\"scope\">\n                        <el-tag size=\"medium\" :type=\"scope.row.state ? 'success' : 'danger'\">{{ scope.row.state ? '已完成' : '未完成' }}</el-tag>          \n                    </template>               \n                </el-table-column>\n                <el-table-column\n                label=\"操作\"\n                width=\"200\">\n                    <template #default=\"scope\">\n                        <el-button size=\"mini\" type=\"danger\" @click=\"deleteItem(scope.$index)\">删除</el-button>\n                        <el-button size=\"mini\" type=\"primary\" @click=\"callUser(scope.row)\">联系客户</el-button>            \n                    </template>     \n                </el-table-column>\n                <el-table-column\n                label=\"支付方式\"\n                width=\"100\">\n                    <template #default=\"scope\">\n                        <el-tag size=\"medium\">{{ scope.row.payType ? '微信' : '支付宝' }}</el-tag>          \n                    </template>\n                </el-table-column>\n                <el-table-column\n                label=\"来源\"\n                width=\"200\"\n                prop=\"source\">\n                </el-table-column>\n            </el-table>\n        </div>\n        \n    </div>\n</template>\n\n<script>\nimport Mock from '../../mock/Mock'\nimport Tools from '../../tools/Tools'\nexport default {\n    data() {\n        return {\n            // 订单列表数据\n            orderList:[],\n            // 筛选订单的参数\n            queryParam:{\n                good:\"\",\n                consignee:\"\",\n                phone:\"\",\n                name:\"\",\n                payTime:\"\",\n                sendTime:\"\"\n            },\n            // 当前选中的订单对象\n            multipleSelection:[]\n        }\n    },\n    mounted () {\n        this.orderList = Mock.getOrder(this.$route.params.type);\n    },\n    // 路由更新时刷新数据\n    beforeRouteUpdate (to) {\n        this.orderList = Mock.getOrder(to.params.type);\n    },\n    methods : {\n        // 模拟请求数据\n        requestData() {\n            this.$message({\n                type:'success',\n                message:'筛选请求参数：' + JSON.stringify(this.queryParam)\n            })\n            this.orderList = Mock.getOrder(this.$route.params.type);\n        },\n        // 切换Tab 刷新数据\n        handleClick(tab) {\n            this.$message({\n                type:'success',\n                message:'切换tab刷新数据：' + tab.props.label\n            })\n            this.orderList = Mock.getOrder(this.$route.params.type);\n        },\n        // 清空筛选项\n        clear() {\n            this.queryParam = {\n                good:\"\",\n                consignee:\"\",\n                phone:\"\",\n                name:\"\",\n                payTime:\"\",\n                sendTime:\"\"\n            }\n            this.orderList = Mock.getOrder(this.$route.params.type);\n        },\n        // 导出订单\n        exportData() {\n            Tools.exportJson('订单.json', JSON.stringify(this.orderList));\n        },\n        // 导出选中的发货单\n        exportDispatchGoods() {\n            Tools.exportJson('发货单.json', JSON.stringify(this.multipleSelection));\n        },\n        // 处理多选\n        handleSelectionChange(val) {\n            this.multipleSelection = val;\n        },\n        // 进行发货\n        dispatchGoods() {\n            this.$message({\n                type:'success',\n                message:'发货商品：' + JSON.stringify(this.multipleSelection)\n            })\n        },\n        // 删除订单\n        deleteItem(item) {\n            this.orderList.splice(item, 1)\n        },\n        // 联系用户\n        callUser(item) {\n            console.log(item)\n            this.$message({\n                type:'success',\n                message:'联系客户：' + item.phone\n            })\n        }\n    }\n}\n</script>\n"],"sourceRoot":""}]}