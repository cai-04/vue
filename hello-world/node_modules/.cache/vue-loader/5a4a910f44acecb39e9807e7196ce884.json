el=\"item.label\"\n          :key=\"item.value\"\n        >\n        </el-option>\n      </el-option-group>\n    </el-select>\n  </div>\n\n  <div style=\"margin-top: 500px\">\n    <el-cascader\n      v-model=\"value\"\n      :options=\"datas\"\n      :props=\"{ expandTrigger: 'hover' }\"\n    ></el-cascader>\n  </div>\n\n  <div id=\"div\">\n    <el-switch\n      v-model=\"switch1\"\n      active-text=\"会员\"\n      inactive-text=\"非会员\"\n      active-color=\"#00FF00\"\n      inactive-color=\"#FF0000\"\n    ></el-switch>\n  </div>\n  <div id=\"div\">\n    <el-switch\n      v-model=\"switch2\"\n      active-text=\"加载中\"\n      :loading=\"true\"\n    ></el-switch>\n  </div>\n  <div id=\"div\">\n    <el-switch\n      v-model=\"switch3\"\n      inactive-text=\"禁用\"\n      :disabled=\"true\"\n    ></el-switch>\n  </div>\n\n  <div style=\"margin: 40px\">\n    <el-slider\n      v-model=\"sliderValue\"\n      :format-tooltip=\"format\"\n      :step=\"10\"\n      :show-stops=\"true\"\n      :show-input=\"true\"\n      :range=\"true\"\n    ></el-slider>\n  </div>\n\n  <div style=\"padding: 40px\">\n    <el-slider v-model=\"sliderValue\" :marks=\"marks\"></el-slider>\n  </div>\n\n  <div>\n    <el-time-picker\n      :is-range=\"true\"\n      v-model=\"time\"\n      range-separator=\"~\"\n      :arrow-control=\"true\"\n      start-placeholder=\"开始时间\"\n      end-placeholder=\"结束时间\"\n    >\n    </el-time-picker>\n  </div>\n\n  <div style=\"margin-top: 500px\">\n    <el-date-picker\n      v-model=\"date\"\n      type=\"daterange\"\n      range-separator=\"至\"\n      start-placeholder=\"开始日期\"\n      end-placeholder=\"结束日期\"\n      >>\n    </el-date-picker>\n  </div>\n\n  <div style=\"margin-top: 300px\">\n    <el-color-picker :show-alpha=\"true\" v-model=\"color\"></el-color-picker>\n  </div>\n\n  <div id=\"div\">\n    <el-alert title=\"成功提示的文案\" type=\"success\"> </el-alert>\n    <br />\n    <el-alert title=\"消息提示的文案\" type=\"info\"> </el-alert>\n    <br />\n    <el-alert title=\"